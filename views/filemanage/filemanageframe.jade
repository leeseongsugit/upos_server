doctypehtml
html
  head
    meta(charset='utf-8')
    title upos_server
    link(rel='stylesheet', href='/stylesheets/frame.css')
    block head

  body(topmargin='5', leftmargin='5')
    div(id = 'frame')
      div(id = "mainmenu", align="left",style='float;width:100;height:30%')
      include ../mainmenu

      div
        div(id = "sidemenu", align="center",style='float:left;width:25%;height:100%')
          include filemanage_menu

        div(id = "contents", align="center",style='clear:right;float:right;width:75%;height:100%')
          block content

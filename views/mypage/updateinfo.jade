extends mypageframe

block content
  form(action='/upload' method='post')
    p
      label 이름:
      div= name
    p
      label 학번:
      div= id
    p
      label 연락처:
      input(type='text' name='phonenumber')
    p
      label 이메일:
      input(type='text' name='email')
    p
      button.btn.btn-lg.btn-primary.btn-block(type="submit") 수정
      button.btn.btn-lg.btn-primary.btn-block(type="reset") 취소
